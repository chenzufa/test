////个人信息

#import <Foundation/Foundation.h>

@interface CustomerInfoEntity : NSObject

@property (retain,nonatomic) NSString *imgurl;//个人头像下载地址
@property (retain,nonatomic) NSString *nickname;//用户昵称
@property (retain,nonatomic) NSString *name;//用户姓名
@property (retain,nonatomic) NSString *sex;//性别

@property (nonatomic) int acounttype;//会员类型：1.普通2….(待与后台商定)

@property (retain,nonatomic) NSString *birthday;//生日
@property (retain,nonatomic) NSString *city;//城市
@property (retain,nonatomic) NSString *mobile;//手机号码
@property (retain,nonatomic) NSString *tel;//联系电话
@property (retain,nonatomic) NSString *qq;//qq号码
@property (retain,nonatomic) NSString *email;//邮箱地址
@property (retain,nonatomic) NSString *totalconsume;//总消费额
@property (retain,nonatomic) NSString *balance;//账户余额
@property (retain,nonatomic) NSString *score;//会员积分

@end
