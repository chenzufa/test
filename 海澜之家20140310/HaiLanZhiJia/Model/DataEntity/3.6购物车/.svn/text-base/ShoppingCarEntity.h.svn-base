//购物车信息

#import <Foundation/Foundation.h>

@interface ShoppingCarEntity : NSObject

@property (nonatomic) int selectcount;//选中的商品件数（一个套装算一件）

@property (retain,nonatomic) NSString *goodsfee;//商品总额
@property (retain,nonatomic) NSString *discount;//优惠金额
@property (retain,nonatomic) NSString *total;//应付总额

@property (nonatomic) int presentationcount;
@property (retain,nonatomic) NSString *presentationinfo;//赠送商品信息（没有赠品信息返回””）
@property (retain,nonatomic) NSArray *discountinfo;//stringArray	优惠信息列表

//NSArray GoodEntity
@property (retain,nonatomic) NSArray *goodslist;//购物车中的商品列表


@end
