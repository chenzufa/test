#import <Foundation/Foundation.h>

@interface GoodEntity : NSObject

@property (retain,nonatomic) NSString *goodsid;//商品id
@property (retain,nonatomic) NSString *goodsimg;//商品预览图片下载地址

@property (retain,nonatomic) NSString *price;//商品价格
@property (retain,nonatomic) NSString *color;//商品有几种颜色 //我的衣柜选择的商品的颜色
//
@property (retain,nonatomic) NSString *goodsname;//商品名称
@property (nonatomic, retain) NSString *sizeandcolor;    //订单详情处使用
//我的衣柜选择的商品
@property (retain,nonatomic) NSString *size;//商品尺寸

//购物车的商品
@property (nonatomic) int isselect;//是否被选中:0.没有选中 
@property (nonatomic) int count;//该商品的数量
@property (retain,nonatomic) NSString *discountprice;//商品优惠价格（该字段对套装有效：组成套装的商品，单独显示时的优惠价格）
@property (retain,nonatomic) NSString *recordid;//购物车中商品记录id


@property (nonatomic) int  ispresentation;//	int	是否赠品 0：不是 1：是的
@end
